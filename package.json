{
  "engines": {
    "node": "10.*"
  },
  "name": "a-house",
  "description": "Webapp for ahouse",
  "version": "3.0.0",
  "main": "src/server/server.js",
  "license": "MIT",
  "scripts": {
    "prod": "NODE_ENV=production concurrently --kill-others \"node _build_prod/bin/server.prod.js\"",
    "dev": "rimraf _build_dev && cross-env NODE_ENV=development concurrently --kill-others \"node bin/server.dev.js\"",
    "buildscript": "cross-env NODE_ENV=production node scripts/build",
    "bundle-analyzer": "webpack-bundle-analyzer _build_prod/client/stats.json",
    "preparecommitmsg": "node scripts/hooks/prepare-commit-msg",
    "preparecommitmsg:doc": "echo 'workaround for windows users'",
    "test": "jest --config jest.config.json --coverage",
    "test:watch": "jest --watch --config jest.config.json",
    "test:update": "jest --config jest.config.json --updateSnapshot",
    "fix": "prettier-eslint --write \"src/**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "prettier-eslint --write",
        "git add"
      ]
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "common-tags": "^1.8.0",
    "compression": "^1.7.3",
    "concurrently": "^4.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "extract-css-chunks-webpack-plugin": "^3.2.1",
    "history": "^4.7.2",
    "http-proxy": "^1.17.0",
    "immutable": "^3.8.2",
    "morgan": "^1.9.1",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-cookie": "^3.0.4",
    "react-dom": "^16.6.1",
    "react-helmet": "^5.2.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.1.0",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "react-universal-component": "^3.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "^2.5.0",
    "shelljs": "^0.8.2",
    "universal-cookie-express": "^3.0.4",
    "uuid": "^3.3.2",
    "webpack-flush-chunks": "^2.0.3",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-universal-import": "^3.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.14.0",
